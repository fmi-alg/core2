include ../Make.options

default :
	-${MAKE} -C bigfloat

test: 
	-${MAKE} -C bigfloat test

moretest: 
	-${MAKE} -C bigfloat moretest	
	
clean:
	-${MAKE} -C bigfloat clean

veryclean:
	-${MAKE} -C bigfloat veryclean
