# =============================================================
# PATHS =======================================================
# =============================================================

CORE_PATH=..
INCLUDE=-I${CORE_PATH}/inc -I${CORE_PATH}/gmp
LIBPATH=-L${CORE_PATH}/lib -L${CORE_PATH}/gmp/.libs

# =============================================================
# FLAGS =======================================================
# =============================================================

clean:
	-${MAKE} -C test clean

veryclean: 
	-${MAKE} -C test veryclean

